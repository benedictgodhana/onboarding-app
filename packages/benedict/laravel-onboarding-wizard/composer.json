{
  "name": "benedict/laravel-onboarding-wizard",
  "description": "Step-based onboarding form wizard for Laravel + Inertia",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Benedict\\OnboardingWizard\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Benedict\\OnboardingWizard\\OnboardingWizardServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0 || ^11.0 || ^12.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

